{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-restricted-globals": 0,
      "import/no-extraneous-dependencies": 0,
      "no-use-before-define": 0,
      "no-console": 0,
      "react/jsx-filename-extension": 0,
      "object-curly-newline": 0,
      "import/prefer-default-export": 0
    }
}
